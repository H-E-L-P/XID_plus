	Journal = {Monthly Notices of the Royal Astronomical Society},
	Number = {3},
	Pages = {2870-2883},
	Title = {HerMES: point source catalogues from Herschel-SPIRE observations II},
	Url = {http://mnras.oxfordjournals.org/content/444/3/2870.abstract},
	Volume = {444},
